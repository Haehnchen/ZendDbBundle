{
    "name": "espend/zend-db-bundle",
    "type": "symfony-bundle",
    "description": "Wraps ZendDb to Symfony2 and Doctrine",
    "keywords": ["Database"],
    "homepage": "http://espend.de",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Espendiller",
            "email": "daniel@espendiller.net"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": ">=2.1,<2.3-dev",
        "zendframework/zend-db": "2.*",
        "doctrine/doctrine-bundle": "*"
    },
    "require-dev": {
        "doctrine/orm": ">=2.2,<3.0,>=2.2.3"
    },
    "autoload": {
        "psr-0": { "espend\\ZendDbBundle": "" }
    },
    "target-dir": "espend/ZendDbBundle"
}
